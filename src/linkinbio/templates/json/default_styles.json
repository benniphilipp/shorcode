{
    "body-page": {
        "default": {
            "width": "100%",
            "max-width": "420px",
            "min-height": "460px",
            "border-radius": "2%",
            "margin": "0 auto",
            "padding-bottom": ".1rem",
            "background-color": "#ffffff"
        }
    },
    "titel":{
        "default":{
            "margin-top": "10px",
            "color": "#000"
        } 
    },
    "header-page": {
        "default": {
            "display": "flex",
            "max-width": "100%",
            "height": "130px",
            "align-items": "center",
            "justify-content": "center",
            "background-color": "#ffffff",
            "color": "#ffffff"
        }
    },
    "image-header": {
        "default": {
            "width": "90px",
            "height": "90px",
            "overflow": "hidden",
            "border-radius": "50%",
            "display": "flex",
            "align-items": "center",
            "justify-content": "center"
        }
    },
    "page-image": {
        "default": {
            "width": "100%",
            "height": "100%",
            "object-fit": "cover"
        }
    },
    "titel-description": {
        "default": {
            "display": "flex",
            "flex-direction": "column",
            "align-items": "center",
            "justify-content": "center",
            "font-size": "1rem"
        }
    },
    "description": {
        "default": {
            "text-align": "center",
            "color": "#000"
        }
    },
    "sozial": {
        "default": {
            "display": "flex",
            "flex-direction": "column",
            "align-items": "center",
            "justify-content": "center"
        }
    },
    "media-icon": {
        "default": {
            "list-style": "none",
            "display": "flex",
            "gap": "0.75rem",
            "margin": "1rem 0 0 0",
            "padding": "0",
            "align-items": "center",
            "justify-content": "center"
        }
    },
    "fa-brands": {
        "default": {
            "color": "#0d6efd",
            "font-size": "1.2rem",
            "cursor": "pointer",
            "transition": "color 0.5s ease"
        },
        "hover":{
            "color": "#dc3545",
            "transition": "color 0.5s ease"
        }
    },
    "main-content": {
        "default": {
            "margin": ".55rem auto",
            "width": "90%"
        }
    },
    "link-page-btn": {
        "default": {
            "display": "inline-block",
            "text-align": "center",
            "text-decoration": "none",
            "padding": "0.475rem 0.75rem",
            "font-size": "1.1rem",
            "width": "100%",
            "margin-top": ".55rem",
            "margin-bottom": ".55rem",
            "border-radius": ".25rem",
            "color": "#ffffff",
            "background-color": "#0d6efd",
            "transition": "color 0.5s ease, background-color 0.5s ease"
        },
        "hover": {
            "color": "#ffffff",
            "background-color": "#dc3545",
            "transition": "color 0.5s ease, background-color 0.5s ease"
        }
    }
}